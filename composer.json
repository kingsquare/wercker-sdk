{
  "name": "kingsquare/wercker-sdk",
  "type": "sdk",
  "description": "Kingsquare PHP client for interacting with Wercker API",
  "keywords": [
    "Wercker",
    "sdk"
  ],
  "homepage": "http://www.kingsquare.nl/",
  "license": "MIT",
  "authors": [{
    "name": "Robin Speekenbrink",
    "email": "robin@kingsquare.nl"
  }],
  "require": {
    "php": "^5.5 || ^7.1",
    "ext-json": "*",
    "ext-curl": "*",
    "guzzlehttp/guzzle": "^6.0"
  },
  "require-dev": {
    "roave/security-advisories": "dev-master",
    "phpstan/phpstan": "^0.10.6"
  },
  "autoload": {
    "psr-4": {
      "Kingsquare\\Wercker\\": "src/"
    }
  },
  "version": "1.0.0",
  "config": {
    "preferred-install": "dist",
    "optimize-autoloader": true
  }
}
